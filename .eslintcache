uantity }] \n      let newCart = [];\n      for (let order of appendedCart) {\n        let orderWithExistingId = newCart.find(i => i.itemId === order.itemId);\n        if (orderWithExistingId) {\n          orderWithExistingId.quantity += order.quantity;\n        }\n        else {\n          newCart.push({itemId: order.itemId, quantity: order.quantity})\n        }\n      }\n      return newCart.filter(order => order.quantity !== 0);\n    })\n  }\n\n  function calculateCartItemCount() {\n    return cart.reduce((total, order) => total + order.quantity, 0);\n  }\n\n  return (\n    <div className=\"App\">\n      <Router>\n        <Navbar cartItemCount={calculateCartItemCount()}/>\n        <Switch>\n          <Route exact path=\"/products\" component={StorePage}/>\n          <Route exact path=\"/products/:itemId\">\n            <ItemPage onAddToCart={handleAddToCart}/>\n          </Route>\n          <Route exact path=\"/cart\">\n            <CartPage \n              cart={cart}\n              onIncrementOrder={id => handleAddToCart(id, 1)}\n              onDecrementOrder={id => handleAddToCart(id, -1)}\n            />\n          </Route>\n          <Route exact path=\"/checkout\" component={CheckoutPage}/>\n\n          <Route component={HomePage}/>\n        </Switch>\n      </Router>\n    </div>\n  );\n}\n\nexport default App;\n","C:\\Users\\liam2\\Webdev\\Projects\\shop-demo\\src\\components\\StorePage.js",[],"C:\\Users\\liam2\\Webdev\\Projects\\shop-demo\\src\\components\\StorePageItem.js",[],"C:\\Users\\liam2\\Webdev\\Projects\\shop-demo\\src\\components\\Navbar.js",[],"C:\\Users\\liam2\\Webdev\\Projects\\shop-demo\\src\\components\\HomePage.js",[],"C:\\Users\\liam2\\Webdev\\Projects\\shop-demo\\src\\components\\ItemPage.js",[],"C:\\Users\\liam2\\Webdev\\Projects\\shop-demo\\src\\components\\ItemImageCarousel.js",[],["52","53"],"C:\\Users\\liam2\\Webdev\\Projects\\shop-demo\\src\\components\\CartPage.js",[],"C:\\Users\\liam2\\Webdev\\Projects\\shop-demo\\src\\component